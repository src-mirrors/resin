<plugin>
  <description></description>
  <groupId>com.caucho</groupId>
  <artifactId>resin-maven-plugin</artifactId>
  <version>@VERSION@</version>
  <goalPrefix>resin</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>run</goal>
      <description>Runs an embedded Resin instance</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <executePhase>package</executePhase>
      <implementation>com.caucho.maven.MavenRun</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <parameters>
        <parameter>
          <name>port</name>
          <type>int</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>contextRoot</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>rootDirectory</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>timeout</name>
          <type>int</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
      </parameters>
      <configuration>
        <contextRoot implementation="java.lang.String">/${project.artifactId}</contextRoot>
        <rootDirectory implementation="java.io.File">${project.build.directory}/${project.artifactId}</rootDirectory>
      </configuration>
    </mojo>
    
    <mojo>
      <goal>jspc</goal>
      <description>Precompiles JSP files</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <executePhase>compile</executePhase>
      <implementation>com.caucho.maven.MavenJspc</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <parameters>
        <parameter>
          <name>rootDirectory</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>config</name>
          <type>java.io.File</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>compiler</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
      </parameters>
      <configuration>
        <rootDirectory implementation="java.io.File">${project.build.directory}/${project.artifactId}</rootDirectory>
      </configuration>
    </mojo>

    <mojo>
      <goal>upload-war</goal>
      <description>Uploads a WAR file to a running Resin instance</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <executePhase>package</executePhase>
      <implementation>com.caucho.maven.MavenUploadWar</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <parameters>
        <parameter>
          <name>port</name>
          <type>int</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>server</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>contextRoot</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>warFile</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>user</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>password</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>commitMessage</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>version</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>stage</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>virtualHost</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>archive</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
        </parameter>
        <parameter>
          <name>writeHead</name>
          <type>java.lang.Boolean</type>
          <defaultValue>true</defaultValue>
          <required>false</required>
          <editable>true</editable>
        </parameter>
      </parameters>
      <configuration>
        <contextRoot implementation="java.lang.String">/${project.build.finalName}</contextRoot>
        <warFile implementation="java.lang.String">${project.build.directory}/${project.build.finalName}.war</warFile>
      </configuration>
    </mojo>

    <mojo>
      <goal>copy-tag</goal>
